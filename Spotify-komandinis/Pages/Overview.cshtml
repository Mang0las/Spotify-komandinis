@page
@model Spotify_komandinis.OverviewModel
@{

    <div class="text-center">
        <p>Prisijungimas sekmingas! @TempData["access_token"]</p>

    </div>

    <div class="text-center">
        <button id="logout-button" class="logOutButton">Log out with Spotify</button>
    </div>
    }
}
@section Scripts{
<script>
    //login button kuris redirectina i spotify ir grazina koda kartu su URL i /Callback
    $(document).ready(function () {
        $("#logout-button").click(function () {
            window.location.href = "/Index";
        });
    });

</script> 
} 
    
